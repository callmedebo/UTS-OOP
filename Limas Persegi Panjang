package sectionb;

/**
 *
 * @author Debora Marianthi
 */
public class LimasPersegiPanjang extends BangunRuang {
    float panjang;
    float lebar;
    float tinggi;
    float rusuksisitegak;
    
    

    @Override
    float keliling(){
        float keliling = 2 * (panjang+lebar) + 4 * rusuksisitegak;
        System.out.println("Keliling Limas Persegi Panjang: " + keliling);
        return keliling;
    }

    @Override
    float volume(){
        float volume = 1_3 * panjang * lebar * tinggi;
        System.out.println("Volume Limas Persegi Panjang: " + volume);
        return volume;
    }
}
