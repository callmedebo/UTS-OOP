package soalnob;

/**
 *
 * @author Debora Marianthi
 */
public class Kubus extends BangunRuang {
    int rusuk;
    
    public Kubus(int rusuk) {
        this.rusuk = rusuk;
    }  
          
    @Override
    public float keliling(){
        return this.rusuk * 12;
    }

    @Override
    public float volume(){
        return this.rusuk * this.rusuk * this.rusuk;
    }
}
